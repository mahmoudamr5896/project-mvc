@{

}
@model List<Instructor>

<table class="table table-bordered table-hover">
    <tr>
        <th>Id </th>
        <th>Name</th>
        <th>Image </th>
        <th>Salary</th>
        <th>Address</th>
        <th>DEpartment</th>
        <th>Courses</th>
    </tr>
    @foreach(var item in Model )
    {
    <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Image</td>
            <td>@item.Salary</td>
            <td>@item.Adress</td>
            <td>@item.Dept_ID</td>
            <td>@item.Course_ID</td>
    </tr>
    }
   
</table>