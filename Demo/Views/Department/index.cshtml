
@{
    ViewData["Title"] = "index";

}


<h1>index</h1>
@model List<Department>
<table class="table table-bordered table-hover">
    <tr>
        <th>ID</th>
        <th>NAME</th>
        <th>MANGER Name</th>
    </tr>
    @foreach(Department item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Manger</td>
        </tr>
    }
</table>